<%- include('partials/header') -%>
<div class="p-5">
  <!-- row 1 -->
  <div class="grid gap-4 lg:grid-cols-3 md:grid-cols-2 ">
      <% for(var i=0; i<property.length; i++) {%>
    <div class="card w-4/5 md:w-full bg-base-100 shadow-xl mx-auto">
      <figure><img src="<%= property[i].images[0].url%>" alt="Shoes" /></figure>
      <div class="card-body">
        <h2 class="card-title"><%= property[i].streetName%>, <%= property[i].postcode.toUpperCase()%></h2>
        <!-- <p><%= property[i].title%></p> -->
        <div class="card-actions justify-end">
          <a href="/property/<%= property[i]._id%>"><button class="btn btn-primary">View Property</button></a>
        </div>
      </div>
    </div>
    <% } %>
  </div>
</div>


<%- include('partials/footer') -%>