<%- include('partials/header') -%>
  <div class="container">
  <div class="mt-5">
    <h2>Add a new review</h2>
    <form action="/reviews/createReview" enctype="multipart/form-data" method="POST">
      <div class="mb-3">
          <label for="address" class="form-label">Property Address</label>
          <input type="text" class="form-control" id="address" name="address">
      </div>
      <div class="mb-3">
        <label for="tenancyDuration" class="form-label">Dates of tenancy</label>
        <input type="date" class="form-control" id="tenancyDuration" name="tenancyDuration"></textarea>
      </div>
      <div class="mb-3">
        <label for="title" class="form-label">Review title</label>
        <textarea class="form-control" id="title" name="title"></textarea>
      </div>
      <div class="mb-3">
        <label for="reviewBody" class="form-label">Review</label>
        <textarea class="form-control" id="reviewBody" name="reviewBody"></textarea>
      </div>
      
      <!-- <div class="mb-3">
        <label for="imgUpload" class="form-label">Image</label>
        <input type="file" class="form-control" id="imageUpload" name="file">
      </div> -->

      <button type="submit" class="btn btn-primary" value="Upload">Submit</button>
    </form>
  </div>
</div>
<%- include('partials/footer') -%>
